lot.getAddressSlot(_IMPLEMENTATION_SLOT).value;\n     }\n     function _setImplementation(address newImplementation) internal {\n         require(newImplementation.code.length > 0);\n         StorageSlot.getAddressSlot(_IMPLEMENTATION_SLOT).value = newImplementation;\n     }\n }\n ```"},"fullyImplemented":true,"linearizedBaseContracts":[55603],"name":"StorageSlot","nameLocation":"1253:11:64","scope":55604,"usedErrors":[],"usedEvents":[]}],"license":"MIT"},"id":64}